{
  "name": "@ryanjoy/vitepress-plugin-group-icons",
  "type": "module",
  "version": "1.0.0",
  "description": "A plugin that allows your code blocks and code groups to display titles and icons.",
  "packageManager": "pnpm@9.15.3",
  "author": {
    "name": "RyanJoy",
    "email": "RyanJoy2002@gmail.com",
    "url": "https://github.com/get1024"
  },
  "license": "MIT",
  "homepage": "https://get1024.github.io/RyanJoy-s_Web/%F0%9F%93%92%E7%AC%94%E8%AE%B0/%F0%9F%91%A8%F0%9F%8F%BC%E2%80%8D%F0%9F%92%BB%E4%B8%AA%E4%BA%BA%E9%A1%B9%E7%9B%AE/%F0%9F%8C%90%E5%8D%9A%E5%AE%A2/%F0%9F%94%8C%E6%8F%92%E4%BB%B6/vitepress-plugin-group-icons.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/get1024/customIntegrations/tree/main/vitepress-plugin-group-icons",
    "directory": "vitepress-plugin-group-icons"
  },
  "keywords": [
    "vitepress",
    "icons",
    "markdown",
    "vitepress-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@iconify-json/logos": "^1.2.3",
    "@iconify-json/vscode-icons": "^1.2.2",
    "@iconify/utils": "^2.1.33"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@antfu/ni": "^0.23.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.16.11",
    "bumpp": "^9.7.1",
    "eslint": "^9.12.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.3.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "3.0.0-rc.11",
    "vite": "^6.0.7",
    "vitepress": "^1.5.0",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
